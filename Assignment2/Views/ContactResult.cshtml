@model ASPNETClass.Models.ContactModel
@{
    ViewBag.Title = "Contact Submission Results";
}

<h2>Thank you for reaching out!</h2>
<p>We received the following details from you:</p>
<ul>
    <li><strong>Name:</strong> @Model.Name</li>
    <li><strong>Email:</strong> @Model.Email</li>
    <li><strong>Message:</strong> @Model.Message</li>
</ul>
